{
  "manifest_version": 2,
  "name": "BME - Bookmarks",
  "version": "1.0",
  "version_name": "release: 1.0 (commit: f3328ab)",
  "description": "BOOTSTRAP ME - bookmarks from git",
  "homepage_url": "https://github.com/wei3er/bme-bookmarks",
  "icons": {
    "48": "/icons/logo-48.png",
    "96": "/icons/logo-96.png"
  },
  "options_ui": {
    "page": "/prefs/options.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "webRequest",
    "<all_urls>",
    "alarms",
    "unlimitedStorage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "bme-bookmarks@wei3er.com"
    }
  },
  "browser_action": {
    "default_icon": "/icons/logo-96.png",
    "default_title": "BME - Bookmarks",
    "default_popup": "/popups/context.html"
  },
  "background": {
    "scripts": [
      "/common.js",
      "/md5.js",
      "/browser-polyfill.js",
      "/background/firefox.js",
      "/background/chrome.js",
      "/background/handler.js"
    ]
  }
}